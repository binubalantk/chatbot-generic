  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "deploy-local": "npm run build && cd dist/chatbot-generic && npm publish --registry http://192.168.1.9:4873/",
